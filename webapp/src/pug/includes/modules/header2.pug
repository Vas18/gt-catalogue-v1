header.header 
    .header-top
        .container
            .d-flex.align-items-center.justify-content-between
                .s-Topbar_b-Location.collapsed(data-toggle="collapse" href="#collapseCity" role="button" aria-expanded="false" aria-controls="collapseCity") 
                    img.mr-1(src="assets/img/placeholder-filled-point.svg", alt="point" width="12px" height="15px")
                    | Москва
                ul.navbar-t
                    each val, index in ['Доставка','Оплата','Гарантия','Контакты']
                        li.navbar-t_item(class=(index==1 || index==2)? 'd-sm-block d-none': '') 
                            a.navbar-t_link(href="#")=val
    .container 
        nav.navbar.navbar-expand-lg.d-flex.justify-content-between.align-items-center 
            .d-flex.align-items-center 
                a.navbar-brand(href='#') ЛОГОТИП
                .d-lg-flex.d-none.align-items-lg-center 
                    .line-vertival
                    .nav-item.active
                        a.nav-link(href='#') ЗКАЗАТЬ ТОВАР
            .d-flex.align-items-center

                ul.navbar-nav.mr-xl-4.pr-xl-3.d-lg-flex.d-none 
                    each val, index in ['ОНЛАЙН ШКОЛА','РЕЦЕПТЫ']
                        li.nav-item
                            a.nav-link(href='#')=val
                        li.nav-item.d-lg-none
                            a.nav-link(href='#') ЗКАЗАТЬ ТОВАР
                a#sign-in.btn.ml-5(href="#") ВОЙТИ
                button.navbar-toggler.collapsed(type='button' data-toggle='collapse' data-target='#navbarNav' aria-controls='navbarNav' aria-expanded='false' aria-label='Toggle navigation') МЕНЮ
                    img.navbar-toggler-icon(src="assets/img/burger.png" alt="burger")


nav#navbarNav.collapse
    .d-flex.align-items-center.justify-content-between.mb-4.pb-1 
        .d-flex
            a.btn.mr-4(href="#") ВОЙТИ
            a.btn-pink(href="#") РЕГИСТРАЦИЯ
        button.close.collapsed(type='button' data-toggle='collapse' data-target='#navbarNav' aria-controls='navbarNav' aria-expanded='false' aria-label='Toggle navigation')
            img(src="assets/img/close-menu.png" alt="close")
